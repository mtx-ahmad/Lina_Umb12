@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
    Layout = "WebBase.cshtml";
    string strheaderImage = "", strheaderImageAlt = "";
    if (Umbraco.AssignedContentItem.HasProperty("headerImage") && Umbraco.AssignedContentItem.Value("headerImage") != null
       && Umbraco.AssignedContentItem.Value<IPublishedContent>("headerImage") != null)
    {
        strheaderImage = Umbraco.AssignedContentItem.Value<IPublishedContent>("headerImage").Url();
    }
    if (Umbraco.AssignedContentItem.HasProperty("headerImageAlt") && Umbraco.AssignedContentItem.Value("headerImageAlt") != null
       && Umbraco.AssignedContentItem.Value<string>("headerImageAlt") != "")
    {
        strheaderImageAlt = Umbraco.AssignedContentItem.Value<string>("headerImageAlt");
    }
    string strheaderImageMobile = "", strheaderImageMobileAlt = "";
    if (Umbraco.AssignedContentItem.HasProperty("headerImageMobile") && Umbraco.AssignedContentItem.Value("headerImageMobile") != null
       && Umbraco.AssignedContentItem.Value<IPublishedContent>("headerImageMobile") != null)
    {
        strheaderImageMobile = Umbraco.AssignedContentItem.Value<IPublishedContent>("headerImageMobile").Url();
    }
    if (Umbraco.AssignedContentItem.HasProperty("headerImageMobileAlt") && Umbraco.AssignedContentItem.Value("headerImageMobileAlt") != null
       && Umbraco.AssignedContentItem.Value<string>("headerImageMobileAlt") != "")
    {
        strheaderImageMobileAlt = Umbraco.AssignedContentItem.Value<string>("headerImageMobileAlt");
    }
    if (strheaderImageMobile == "")
    {
        strheaderImageMobile = strheaderImage;
        strheaderImageMobileAlt = strheaderImageAlt;
    }
    string strcenterText = "";
    if (Umbraco.AssignedContentItem.HasProperty("centerText") && Umbraco.AssignedContentItem.Value("centerText") != null
       && Umbraco.AssignedContentItem.Value<string>("centerText") != "")
    {
        strcenterText = Umbraco.AssignedContentItem.Value<string>("centerText");
    }
    //Content
    string strleftImage = "", strleftImageAlt = "";
    if (Umbraco.AssignedContentItem.HasProperty("leftImage") && Umbraco.AssignedContentItem.Value("leftImage") != null
       && Umbraco.AssignedContentItem.Value<IPublishedContent>("leftImage") != null)
    {
        strleftImage = Umbraco.AssignedContentItem.Value<IPublishedContent>("leftImage").Url();
    }
    if (Umbraco.AssignedContentItem.HasProperty("leftImageAlt") && Umbraco.AssignedContentItem.Value("leftImageAlt") != null
       && Umbraco.AssignedContentItem.Value<string>("leftImageAlt") != "")
    {
        strleftImageAlt = Umbraco.AssignedContentItem.Value<string>("leftImageAlt");
    }
    string strleftCenterText = "";
    if (Umbraco.AssignedContentItem.HasProperty("leftCenterText") && Umbraco.AssignedContentItem.Value("leftCenterText") != null
       && Umbraco.AssignedContentItem.Value<string>("leftCenterText") != "")
    {
        strleftCenterText = Umbraco.AssignedContentItem.Value<string>("leftCenterText");
    }
    string strleftSmallImage = "", strleftSmallImageAlt = "";
    if (Umbraco.AssignedContentItem.HasProperty("leftSmallImage") && Umbraco.AssignedContentItem.Value("leftSmallImage") != null
       && Umbraco.AssignedContentItem.Value<IPublishedContent>("leftSmallImage") != null)
    {
        strleftSmallImage = Umbraco.AssignedContentItem.Value<IPublishedContent>("leftSmallImage").Url();
    }
    if (Umbraco.AssignedContentItem.HasProperty("leftSmallImageAlt") && Umbraco.AssignedContentItem.Value("leftSmallImageAlt") != null
       && Umbraco.AssignedContentItem.Value<string>("leftSmallImageAlt") != "")
    {
        strleftSmallImageAlt = Umbraco.AssignedContentItem.Value<string>("leftSmallImageAlt");
    }
    string strleftBelowText = "";
    if (Umbraco.AssignedContentItem.HasProperty("leftBelowText") && Umbraco.AssignedContentItem.Value("leftBelowText") != null
       && Umbraco.AssignedContentItem.Value<string>("leftBelowText") != "")
    {
        strleftBelowText = Umbraco.AssignedContentItem.Value<string>("leftBelowText");
    }
    //Left and right image with center text
    string strleftPicturel = "", strleftPictureAlt = "";
    if (Umbraco.AssignedContentItem.HasProperty("leftPicture") && Umbraco.AssignedContentItem.Value("leftPicture") != null
       && Umbraco.AssignedContentItem.Value<IPublishedContent>("leftPicture") != null)
    {
        strleftPicturel = Umbraco.AssignedContentItem.Value<IPublishedContent>("leftPicture").Url();
    }
    if (Umbraco.AssignedContentItem.HasProperty("leftPictureAlt") && Umbraco.AssignedContentItem.Value("leftPictureAlt") != null
       && Umbraco.AssignedContentItem.Value<string>("leftPictureAlt") != "")
    {
        strleftPictureAlt = Umbraco.AssignedContentItem.Value<string>("leftPictureAlt");
    }
    string strrightPicture = "", strrightPictureAlt = "";
    if (Umbraco.AssignedContentItem.HasProperty("rightPicture") && Umbraco.AssignedContentItem.Value("rightPicture") != null
       && Umbraco.AssignedContentItem.Value<IPublishedContent>("rightPicture") != null)
    {
        strrightPicture = Umbraco.AssignedContentItem.Value<IPublishedContent>("rightPicture").Url();
    }
    if (Umbraco.AssignedContentItem.HasProperty("rightPictureAlt") && Umbraco.AssignedContentItem.Value("rightPictureAlt") != null
       && Umbraco.AssignedContentItem.Value<string>("rightPictureAlt") != "")
    {
        strrightPictureAlt = Umbraco.AssignedContentItem.Value<string>("rightPictureAlt");
    }
    var strcenterInfo = "";
    if (Umbraco.AssignedContentItem.HasProperty("centerInfo") && Umbraco.AssignedContentItem.Value("centerInfo") != null
       && Umbraco.AssignedContentItem.Value<string>("centerInfo") != "")
    {
        strcenterInfo = Umbraco.AssignedContentItem.Value<string>("centerInfo");
    }

    string strrightText = "";
    if (Umbraco.AssignedContentItem.HasProperty("rightText") && Umbraco.AssignedContentItem.Value("rightText") != null
       && Umbraco.AssignedContentItem.Value<string>("rightText") != "")
    {
        strrightText = Umbraco.AssignedContentItem.Value<string>("rightText");
    }
    string strrightLargeImage = "", strrightLargeImageAlt = "";
    if (Umbraco.AssignedContentItem.HasProperty("rightLargeImage") && Umbraco.AssignedContentItem.Value("rightLargeImage") != null
       && Umbraco.AssignedContentItem.Value<IPublishedContent>("rightLargeImage") != null)
    {
        strrightLargeImage = Umbraco.AssignedContentItem.Value<IPublishedContent>("rightLargeImage").Url();
    }
    if (Umbraco.AssignedContentItem.HasProperty("rightLargeImageAlt") && Umbraco.AssignedContentItem.Value("rightLargeImageAlt") != null
       && Umbraco.AssignedContentItem.Value<string>("rightLargeImageAlt") != "")
    {
        strrightLargeImageAlt = Umbraco.AssignedContentItem.Value<string>("rightLargeImageAlt");
    }
    //Left and right image with text
    string strleftLargeImage = "", strleftLargeImageAlt = "";
    if (Umbraco.AssignedContentItem.HasProperty("leftLargeImage") && Umbraco.AssignedContentItem.Value("leftLargeImage") != null
       && Umbraco.AssignedContentItem.Value<IPublishedContent>("leftLargeImage") != null)
    {
        strleftLargeImage = Umbraco.AssignedContentItem.Value<IPublishedContent>("leftLargeImage").Url();
    }
    if (Umbraco.AssignedContentItem.HasProperty("leftLargeImageAlt") && Umbraco.AssignedContentItem.Value("leftLargeImageAlt") != null
       && Umbraco.AssignedContentItem.Value<string>("leftLargeImageAlt") != "")
    {
        strleftLargeImageAlt = Umbraco.AssignedContentItem.Value<string>("leftLargeImageAlt");
    }
    string strleftLargeImageText = "";
    if (Umbraco.AssignedContentItem.HasProperty("leftLargeImageText") && Umbraco.AssignedContentItem.Value("leftLargeImageText") != null
