    }
    string strsmallImageText = "";
    if (Umbraco.AssignedContentItem.HasProperty("smallImageText") && Umbraco.AssignedContentItem.Value("smallImageText") != null
       && Umbraco.AssignedContentItem.Value<string>("smallImageText") != "")
    {
        strsmallImageText = Umbraco.AssignedContentItem.Value<string>("smallImageText");
    }
}
@if (!string.IsNullOrEmpty(strheaderImage))
{
    <section class="intro-img-box small">
        <figure class="pattern web" style="background-image:url(@strheaderImage);background-position: top center;"></figure>
        <img src="@strheaderImage" alt="@strheaderImageAlt" style="display:none;" />
        <figure class="pattern mobile" style="background-image:url(@strheaderImageMobile);background-position: top center;"></figure>
        <img src="@strheaderImageMobile" alt="@strheaderImageMobileAlt" style="display:none;" />
        @if (!string.IsNullOrEmpty(strheaderImage))
        {
            <div class="description">
                <div><h1>@strcenterText</h1></div>
        </div>
        }
    </section>
}
<section class="finishes-box">
    <div class="inner">
        <article class="s1">
            @if (!string.IsNullOrEmpty(strrightText))
            {
                <p class="fs">@strrightText</p>
            }
            <div class="img-1 lm70">
                @if (!string.IsNullOrEmpty(strleftImage))
                {
                    <figure><img alt="@strleftImageAlt" src="@strleftImage" /></figure>
                }
                @if (!string.IsNullOrEmpty(strleftCenterText))
                {
                    <p>@strleftCenterText</p>
                }
                @if (!string.IsNullOrEmpty(strleftSmallImage))
                {
                    <figure class="small"><img alt="@strleftSmallImageAlt" src="@strleftSmallImage" /></figure>
                }
                @if (!string.IsNullOrEmpty(strleftBelowText))
                {
                    <p>@strleftBelowText</p>
                }
            </div>
         
            @if (!string.IsNullOrEmpty(strrightLargeImage))
                {
            <div class="img-2 rt">
                    <figure><img alt="@strrightLargeImageAlt" src="@strrightLargeImage" /></figure>
            </div>
                }
        </article>
        <article class="s3">
            <div class="img-8">
                @if (!string.IsNullOrEmpty(strleftPicturel))
                {
                    <figure><img alt="@strleftPictureAlt" src="@strleftPicturel" /></figure>
                }
                @if (!string.IsNullOrEmpty(strcenterInfo))
                {
                    <p>@strcenterInfo</p>
                }
            </div>
            @if (!string.IsNullOrEmpty(strrightPicture))
                {
            <div class="img-5 rt rm70">
                    <figure><img alt="@strrightPictureAlt" src="@strrightPicture" /></figure>
            </div>
                }
        </article>
        <article class="s2">
            <div class="img-3 lm70">
                @if (!string.IsNullOrEmpty(strleftLargeImage))
                {
                    <figure>
                        <img alt="@strleftLargeImageAlt" src="@strleftLargeImage" />
                    </figure>
                }
                @if (!string.IsNullOrEmpty(strleftLargeImageText))
                {
                    <p>@strleftLargeImageText</p>
                }
            </div>
            @*@if (!string.IsNullOrEmpty(strrightSmallImage))
                {
            <div class="img-5 rt rm70">
                    <figure><img alt="@strrightSmallImageAlt" src="@strrightSmallImage" /></figure>
            </div>
                }*@
        </article>
        <article>
            <div class="img-9">
                @if (!string.IsNullOrEmpty(strsmallImage))
                {
                    <figure><img alt="@strsmallImageAlt" src="@strsmallImage" /></figure>
                } 
               @* @if (!string.IsNullOrEmpty(strsmallImageText))
                {
                    <p>@strsmallImageText</p>
                }*@
            </div>
        </article>
    </div>
</section>
