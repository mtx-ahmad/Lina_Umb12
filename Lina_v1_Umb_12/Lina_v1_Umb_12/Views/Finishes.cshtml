       && Umbraco.AssignedContentItem.Value<IPublishedContent>("rightSmallImage") != null)
    {
        strrightSmallImage = Umbraco.AssignedContentItem.Value<IPublishedContent>("rightSmallImage").Url();
    }
    if (Umbraco.AssignedContentItem.HasProperty("rightSmallImageAlt") && Umbraco.AssignedContentItem.Value("rightSmallImageAlt") != null
       && Umbraco.AssignedContentItem.Value<string>("rightSmallImageAlt") != "")
    {
        strrightSmallImageAlt = Umbraco.AssignedContentItem.Value<string>("rightSmallImageAlt");
    }
    string strsmallImage = "", strsmallImageAlt = "";
    if (Umbraco.AssignedContentItem.HasProperty("smallImage") && Umbraco.AssignedContentItem.Value("smallImage") != null
       && Umbraco.AssignedContentItem.Value<IPublishedContent>("smallImage") != null)
    {
        strsmallImage = Umbraco.AssignedContentItem.Value<IPublishedContent>("smallImage").Url();
    }
    if (Umbraco.AssignedContentItem.HasProperty("smallImageAlt") && Umbraco.AssignedContentItem.Value("smallImageAlt") != null
       && Umbraco.AssignedContentItem.Value<string>("smallImageAlt") != "")
    {
